{
  "name": "@godgiven/gui-form-builder",
  "description": "Godgiven form builder",
  "version": "0.0.1",
  "main": "./dist/index.js",
  "type": "module",
  "private": true,
  "scripts": {
    "b": "build",
    "w": "watch",
    "s": "serve",
    "c": "clean",
    "build": "npm-run-all lint build:*",
    "build:ts": "tsc --build",
    "lint": "npm-run-all lint:*",
    "lint:ts": "eslint src/**/*.ts",
    "prepare": "yarn build",
    "serve": "web-dev-server --watch --config dev-server.mjs",
    "watch": "npm-run-all --parallel lint watch:* serve",
    "watch:ts": "yarn build:ts --watch",
    "clean": "tsc --build --clean"
  },
  "dependencies": {
    "lit-element": "^2.4.0",
    "tslib": "^2.1.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "@web/dev-server": "^0.1.8",
    "@webcomponents/webcomponentsjs": "^2.5.0",
    "eslint": "^7.22.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-config-standard-with-typescript": "^20.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^32.3.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-wc": "^1.3.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.2.3"
  },
  "author": "Godgiven",
  "license": "Apache-2.0"
}
